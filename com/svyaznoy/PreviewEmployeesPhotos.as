package com.svyaznoy {	import flash.display.Sprite;		/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class PreviewEmployeesPhotos extends PreviewItem {				private var previewGallery:PreviewGallery = new PreviewGallery();				/**		 * 		 */				public function PreviewEmployeesPhotos() {			previewGallery.isPreviewEmployesPhotos = true;			addChild( previewGallery );		}				/**		 * 		 */				override public function dispose():void {			super.dispose();			if ( previewGallery ) {				previewGallery.dispose();				removeChild( previewGallery );				previewGallery = null;			}		}				/**		 * 		 * @param	data		 */				override public function displayData( data:Object ):void {			super.displayData( data );			previewGallery.displayData( data );		}				/**		 * 		 */				override public function get width():Number {			return previewGallery.width;		}				override public function set width( value:Number ):void {			previewGallery.width = value;		}				/**		 * 		 */				override public function get height():Number {			return previewGallery.height;		}				override public function set height( value:Number ):void {			previewGallery.height = value;		}			}}