package com.svyaznoy {		/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */		public class SettingsData {				private var data:Object;		private var _showAnnouncementNeeded:Boolean;		private var _achivements:AchievementsData;		private var _starsInLightning:int;				/**		 * 		 * @param	data		 */				public function SettingsData( data:Object ) {			this.data = data;			_achivements = new AchievementsData( data.achivements );		}				/**		 * 		 */				public function get showAnnouncementNeeded():Boolean {			return Boolean( int( data.show_anonces ) );		}				/**		 * 		 */				public function get achievements():AchievementsData {			return _achivements;		}				/**		 * 		 */				public function get starsInLightning():int {			return data.achivements.stars_in_battery;		}			}}