package com.svyaznoy {		/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class ProfilePhotosDepartureBar extends DroppedPhotosBar {				private const ROW_COUNT:int = 3;		private const TITLE_LENGTH_NAME:String = "фото";				private var departureID:String;				/**		 * 		 * @param	datasList		 */				public function ProfilePhotosDepartureBar( datasList:Vector.<Object>, loadPhotos:Boolean = true ) {			departureID = datasList[ 0 ].departure_id;			super( ROW_COUNT, datasList, loadPhotos, PreviewDepartureImage, datasList[ 0 ].departureName, TITLE_LENGTH_NAME );		}				/**		 * 		 * @return		 */				public function getDepartureID():String {			return departureID;		}			}}