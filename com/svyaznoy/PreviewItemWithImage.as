package com.svyaznoy {	/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class PreviewItemWithImage extends PreviewItem {				protected var previewImage:PreviewImage;				/**		 * 		 */				public function PreviewItemWithImage() {			previewImage = getChildByName( "previewImage_mc" ) as PreviewImage;		}				override public function dispose():void {			super.dispose();			previewImage.dispose();		}				/**		 * 		 * @param	data		 */				override public function displayData( data:Object ):void {			super.displayData( data );						if( data.title ) previewImage.title = data.title;			previewImage.description = data.anonce;		}			}}