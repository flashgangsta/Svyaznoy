package com.svyaznoy {	/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class UserData {				private var _username:String;		private var _created_at:String;		private var _id:String;		private var _updated_at:String;		private var _employee:Boolean;		private var _intro:Boolean;		private var _employeIsNotFired:Boolean = false;		private var _firstName:String;		private var _lastName:String;		private var _middleName:String;		private var _fullName:String;		private var _nameAndSurname:String;		private var _employeeID:String;		private var _photo50:String = "http://cs320931.vk.me/v320931535/f98/7VTxSYuOW-M.jpg";		private var _photo100:String = "http://cs320931.vk.me/v320931535/f97/WDynbN3YWYU.jpg";		private var _photo200:String = "http://cs320931.vk.me/v320931535/f96/jLXhHlzGoRM.jpg";				/**		 * 		 * @param	data		 */				public function UserData( data:Object ) {			if ( data.hasOwnProperty( "employee" ) && data.employee !== null ) {				/* Response from server example:				 * {					"id": "2",					"username": "7010535",					"email": "7010535@vk.com",					"created_at": "2013-08-08 18:48:17",					"updated_at": "2013-08-08 18:48:17",					"intro": "1",					"employee": {						"id": "3",						"user_id": "2",						"barcode": "1",						"first_name": "Сергей",						"middle_name": "Владимирович",						"last_name": "Молофеев",						"email": "flashgangsta@gmail.com",						"status": "1",						"created_at": "2013-08-08 18:08:01",						"updated_at": "2013-08-08 19:02:31"					}				}*/				var employeData:Object = data.employee;				_employee = true;				_employeIsNotFired = Boolean( employeData.status );				_firstName = employeData.first_name;				_lastName = employeData.last_name;				_middleName = employeData.middle_name;								_username = data.username;				_created_at = data.created_at;				_id = data.id;				_updated_at = data.updated_at;				_intro = Boolean( int( data.intro ) );				_employeeID = employeData.id;			} else if ( data.hasOwnProperty( "user" ) && data.user !== null ) {				/* Response from server example:				 * {					"first_name":"Сергей",					"created_at":"2013-08-08 18:08:01",					"barcode":"1",					"last_name":"Молофеев",					"email":"flashgangsta@gmail.com",					"middle_name":"Владимирович",					"updated_at":"2013-08-08 18:48:39",					"id":"3",					"user_id":"2",					"status":"1",					"user": {						"intro":"1",						"username":"7010535",						"created_at":"2013-08-08 18:48:17",						"email":"7010535@vk.com",						"updated_at":"2013-08-08 18:48:17",						"id":"2" 					}				}*/				var userData:Object = data.user;				_employee = true;				_employeIsNotFired = Boolean( data.status );				_firstName = data.first_name;				_lastName = data.last_name;				_middleName = data.middle_name;				_intro = Boolean( int( userData.intro ) );				_username = userData.username;				_created_at = data.created_at;				_id = userData.id;				_updated_at = data.updated_at;				_employeeID = id;			} else {				/* Response from server example:				 * {					"id": "2",					"username": "7010535",					"email": "7010535@vk.com",					"created_at": "2013-08-08 18:48:17",					"updated_at": "2013-08-08 18:48:17",					"intro": "1",				}*/								_username = data.username;				_created_at = data.created_at;				_id = data.id;				_updated_at = data.updated_at;				_intro = Boolean( int( data.intro ) );			}						_fullName = setFullName( _firstName, _lastName, _middleName );			_nameAndSurname = setFullName( _firstName, _lastName );		}				/**		 * 		 */				public function get username():String {			return _username;		}				/**		 * 		 */				public function get created_at():String {			return _created_at;		}				/**		 * 		 */				public function get id():String {			return _id;		}				/**		 * 		 */				public function get updated_at():String {			return _updated_at;		}				/**		 * 		 */				public function get employee():Boolean {			return _employee;		}				/**		 * 		 */				public function get intro():Boolean {			return _intro;		}				/**		 * 		 */				public function get fullName():String {			return _fullName;		}				/**		 * 		 */				public function get employeIsNotFired():Boolean {			return _employeIsNotFired;		}				/**		 * 		 */				public function get firstName():String {			return _firstName;		}				/**		 * 		 */				public function get lastName():String {			return _lastName;		}				/**		 * 		 */				public function get middleName():String {			return _middleName;		}				/**		 * 		 */				public function get employeeID():String {			return _employeeID;		}				/**		 * 		 */				public function get nameAndSurname():String {			return _nameAndSurname;		}				/**		 * 		 */				public function get photo100():String {			return _photo100;		}				/**		 * 		 */				public function get photo200():String {			return _photo200;		}				/**		 * 		 * @param	firstName		 * @param	lastName		 * @param	middleName		 * @return		 */				public function setFullName( firstName:String, lastName:String, middleName:String = null ):String {			_firstName = firstName;			_lastName = lastName;			_fullName = lastName + " " + firstName;						if ( middleName ) {				_middleName = middleName;				_fullName += " " + middleName;			}						return _fullName;		}				/**		 * 		 * @param	photo100		 * @param	photo200		 */				public function setAvatars( photo50:String, photo100:String, photo200:String ):void {			_photo50 = photo50;			_photo100 = photo100;			_photo200 = photo200;		}			}}